<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fuhr.blackjack"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/blackjack"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".BlackJackMain"
            android:label="@string/title_activity_black_jack_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NovoJogo"
            android:label="@string/title_activity_novo_jogo" >
        </activity>
        <activity
            android:name=".Ranking"
            android:label="@string/title_activity_ranking" >
        </activity>
        <activity
            android:name=".BlackJack"
            android:label="@string/title_activity_black_jack" >
        </activity>
        
    </application>
<uses-permission android:name="android.permission.INTERNET" />
</manifest>